{
  "runtime": {
    "version": "LuaJIT",
    "path": ["lua/?.lua", "lua/?/init.lua"]
  },
  "diagnostics": {
    "globals": ["vim"]
  },
  "workspace": {
    "library": ["$VIMRUNTIME/lua", "${3rd}/luv/library", "${3rd}/busted/library"],
    "checkThirdParty": false
  },
  "completion": {
    "callSnippet": "Replace"
  }
}
